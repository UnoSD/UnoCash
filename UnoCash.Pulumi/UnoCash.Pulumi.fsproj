  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Pulumi.Azure" Version="3.7.0" />
    <PackageReference Include="Pulumi.FSharp" Version="2.3.0" />
    <PackageReference Include="FSharp.Core" Version="4.7.1" />
  </ItemGroup>

</Project>
