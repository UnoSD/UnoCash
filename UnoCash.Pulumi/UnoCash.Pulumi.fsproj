  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Pulumi.Azure" Version="3.13.0" />
    <PackageReference Include="Pulumi.AzureAD" Version="2.4.0" />
    <PackageReference Include="Pulumi.FSharp" Version="2.7.1" />
    <PackageReference Include="FSharp.Core" Version="4.7.2" />
